public class Player
{
    static int hp;
    static int atk;
    static int def;
    static int mp;
    static int mpregen;
    static int acc;
    static int exp;
    static int expreq;
    static int level;
    static String name;
        public static void Stats(int a, int b, int c, int d, int e, int f, int g, int h, int i, String j) {
        hp = a;
        atk = b;
        def = c;
        mp = d;
        mpregen = e;
        acc = f;
        exp = g;
        expreq = h;
        level = i;
        name = j;
    }
}
