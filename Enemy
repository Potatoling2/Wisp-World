public class Enemy
{
    static int hp;
    static int atk;
    static int def;
    static int acc;
    static int aggro;
    static int exp;
    static int money;
    static String name;
        public void Attributes(int a, int b, int c, int d, int e, int f, int g, String h) {
        hp = a;
        atk = b;
        def = c;
        acc = d;
        aggro = e;
        exp = f;
        money = g;
        name = h;
    }
}
